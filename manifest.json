{
  "manifest_version": 2,
 
  "name": "GiftR",
  "description": "This extension keeps track of gifting occasions and gifts",
  "version": "0.1.1",
  "icons": { "16": "GiftR_16.png",
           "48": "GiftR_48.png",
          "128": "GiftR_128.png" },
  "browser_action": {
   "default_popup": "popup.html"
  },
  "background" : {
    "scripts": ["jquery-2.1.4.min.js",
     "popup.js"]},
  "permissions": [
   "tabs",
   "activeTab",
   "storage"
   ]

}